{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#180e2090",
                "editor.background": "#180e2090",
                "editor.foreground": "#c5c2c7",
                "text": "#c5c2c7",
                "text.muted": "#c5c2c770",
                "text.ignored": "#c5c2c740",
                "text.placeholder": "#c5c2c750",
                "ignored": "#c5c2c730",
                "element.hover": "#c5c2c730",
                "ghost_element.hover": "#74489C30",
                "ghost_element.selected": "#74489C30",
                "ghost_element.active": "#74489C60",
                "border": "#180e20",
                "editor.highlighted_line.background": "#74489C10",
                "editor.active_line.background": "#74489C10",
                "panel.background": "#180e2090",
                "title_bar.background": "#180e2090",
                "title_bar.inactive_background": "#180e2090",
                "status_bar.background": "#180e2090",
                "drop_target.background": "#180e2090",
                "elevated_surface.background": "#180e20",
                "toolbar.background": "#180e2090",
                "tab_bar.background": "#180e2090",
                "tab.inactive_background": "#180e2090",
                "tab.active_background": "#74489C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#180e20",
                "scrollbar.thumb.background": "#74489C",
                "editor.gutter.background": "#180e2090",
                "terminal.background": "#180e2010",
                "terminal.foreground": "#c5c2c7",
                "terminal.dim_foreground": "#c5c2c7",
                "terminal.bright_foreground": "#c5c2c7",
                "terminal.ansi.black": "#180e20",
                "terminal.ansi.red": "#74489C",
                "terminal.ansi.green": "#7953C5",
                "terminal.ansi.yellow": "#A24D9F",
                "terminal.ansi.blue": "#9655A7",
                "terminal.ansi.magenta": "#A666D5",
                "terminal.ansi.cyan": "#CD73E4",
                "terminal.ansi.white": "#c5c2c7",
                "terminal.ansi.bright_black": "#675d72",
                "terminal.ansi.bright_red": "#74489C",
                "terminal.ansi.bright_green": "#7953C5",
                "terminal.ansi.bright_yellow": "#A24D9F",
                "terminal.ansi.bright_blue": "#9655A7",
                "terminal.ansi.bright_magenta": "#A666D5",
                "terminal.ansi.bright_cyan": "#CD73E4",
                "terminal.ansi.bright_white": "#c5c2c7",
                "modified": "#74489C",
                "syntax": {
                    "attribute": {
                        "color": "#74489C"
                    },
                    "boolean": {
                      "color": "#7953C5"
                    },
                    "comment": {
                      "color": "#675d72"
                    },
                    "comment.doc": {
                      "color": "#675d72"
                    },
                    "constant": {
                      "color": "#7953C5"
                    },
                    "function": {
                      "color": "#CD73E4"
                    },
                    "keyword": {
                      "color": "#9655A7"
                    },
                    "number": {
                      "color": "#A666D5"
                    },
                    "operator": {
                      "color": "#9655A7"
                    },
                    "string": {
                      "color": "#74489C"
                    },
                    "variable": {
                      "color": "#7953C5"
                    }
                },
                "players": [
                  {
                    "cursor": "#74489C",
                    "background": "#180e20",
                    "selection": "#74489C30"
                  }
                ]
            }
        }
    ]
}
