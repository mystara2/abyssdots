                "terminal.ansi.bright_cyan": "#CBB7F9",
                "terminal.ansi.bright_white": "#c3c3c8",
                "modified": "#5551A7",
                "syntax": {
                    "attribute": {
                        "color": "#5551A7"
                    },
                    "boolean": {
                      "color": "#6E6BCF"
                    },
                    "comment": {
                      "color": "#5e6074"
                    },
                    "comment.doc": {
                      "color": "#5e6074"
                    },
                    "constant": {
                      "color": "#6E6BCF"
                    },
                    "function": {
                      "color": "#CBB7F9"
                    },
                    "keyword": {
                      "color": "#9A8AB7"
                    },
                    "number": {
                      "color": "#A099EF"
                    },
                    "operator": {
                      "color": "#9A8AB7"
                    },
                    "string": {
                      "color": "#5551A7"
                    },
                    "variable": {
                      "color": "#6E6BCF"
                    }
                },
                "players": [
                  {
                    "cursor": "#5551A7",
                    "background": "#0f1124",
                    "selection": "#5551A730"
