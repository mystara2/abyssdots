                "scrollbar.thumb.background": "#5551A7",
                "editor.gutter.background": "#0f112490",
                "terminal.background": "#0f112410",
                "terminal.foreground": "#c3c3c8",
                "terminal.dim_foreground": "#c3c3c8",
                "terminal.bright_foreground": "#c3c3c8",
                "terminal.ansi.black": "#0f1124",
                "terminal.ansi.red": "#5551A7",
                "terminal.ansi.green": "#6E6BCF",
                "terminal.ansi.yellow": "#837BD8",
                "terminal.ansi.blue": "#9A8AB7",
                "terminal.ansi.magenta": "#A099EF",
                "terminal.ansi.cyan": "#CBB7F9",
                "terminal.ansi.white": "#c3c3c8",
                "terminal.ansi.bright_black": "#5e6074",
                "terminal.ansi.bright_red": "#5551A7",
                "terminal.ansi.bright_green": "#6E6BCF",
