{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f112490",
                "editor.background": "#0f112490",
                "editor.foreground": "#c3c3c8",
                "text": "#c3c3c8",
                "text.muted": "#c3c3c870",
                "text.ignored": "#c3c3c840",
                "text.placeholder": "#c3c3c850",
                "ignored": "#c3c3c830",
                "element.hover": "#c3c3c830",
                "ghost_element.hover": "#5551A730",
                "ghost_element.selected": "#5551A730",
                "ghost_element.active": "#5551A760",
                "border": "#0f1124",
                "editor.highlighted_line.background": "#5551A710",
                "editor.active_line.background": "#5551A710",
                "panel.background": "#0f112490",
                "title_bar.background": "#0f112490",
                "title_bar.inactive_background": "#0f112490",
                "status_bar.background": "#0f112490",
                "drop_target.background": "#0f112490",
                "elevated_surface.background": "#0f1124",
                "toolbar.background": "#0f112490",
                "tab_bar.background": "#0f112490",
                "tab.inactive_background": "#0f112490",
                "tab.active_background": "#5551A730",
